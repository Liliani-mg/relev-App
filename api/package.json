{
  "name": "app-backend",
  "version": "1.0.1",
  "description": "Earn time with this powerful, scalable and easy to config boilerplate.",
  "author": "Liliani-mg",
  "license": "MIT",
  "homepage": "https://github.com/Liliani-mg/relev-app#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Liliani-mg/relev-app"
  },
  "scripts": {
    "start": "nodemon app.js",
    "test": "cross-env NODE_ENV=test mocha ./test/*.js --exit",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:reset": "npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all --seeders-path ./database/seeders && npm start",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize db:create"
  },
  "engines": {
    "node": ">=0.14"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "express-validator": "^6.13.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.20",
    "sequelize": "^6.19.0",
    "sequelize-cli": "6.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "7.32",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "husky": "7.0.0",
    "jest": "27.0.6",
    "lint-staged": "11.1.1",
    "mocha": "^9.1.3",
    "prettier": "2.3.2",
    "sequelize-cli": "6.2.0",
    "supertest": "^6.1.6"
  }
}
